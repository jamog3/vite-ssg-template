{
  "name": "vite-ssg-template",
  "version": "1.0.0",
  "author": "jamog3",
  "private": true,
  "browserslist": [
    "last 4 version",
    "> 0.5% in JP"
  ],
  "prepare": "husky install",
  "lint-staged": {
    "*.{vue,ts}": [
      "yarn lint:script --fix",
      "yarn lint:style --fix"
    ]
  },
  "scripts": {
    "dev": "cross-env development=vite-ssg:* vite",
    "build": "cross-env production=vite-ssg:* vite-ssg build",
    "serve": "vite preview",
    "lint:markup": "vue-tsc --noEmit",
    "lint:script": "eslint --ext .ts,vue .",
    "lint:style": "stylelint src/**/*.{css,vue}",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "dependencies": {
    "pinia": "2.0.11",
    "vite-ssg": "^0.17.10",
    "vue": "^3.2.31",
    "vue3-smooth-scroll": "^0.8.1",
    "what-input": "^5.2.10"
  },
  "devDependencies": {
    "@nuxt/devalue": "^2.0.0",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@vitejs/plugin-vue": "^2.2.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vueuse/head": "^0.7.5",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.17",
    "cssnano-preset-advanced": "^5.1.12",
    "eslint": "^8.9.0",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "path": "^0.12.7",
    "postcss": "^8.4.7",
    "postcss-html": "^1.3.0",
    "postcss-import": "^14.0.2",
    "postcss-preset-env": "^7.4.1",
    "postcss-rem": "^2.0.2",
    "postcss-reporter": "^7.0.5",
    "postcss-sort-media-queries": "^4.2.1",
    "postcss-webfont": "^4.0.0",
    "prettier": "^2.5.1",
    "stylelint": "^14.5.3",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-property-sort-order-smacss": "^9.0.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "the-new-css-reset": "^1.4.9",
    "typescript": "^4.5.5",
    "unplugin-vue-components": "^0.17.21",
    "vite": "^2.8.4",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-pages": "^0.20.2",
    "vite-plugin-vue-layouts": "^0.6.0",
    "vue-router": "^4.0.12",
    "vue-tsc": "^0.32.0"
  },
  "volta": {
    "node": "16.13.2"
  }
}

{
  "name": "vite-ssg-template",
  "version": "1.0.0",
  "author": "jamog3",
  "private": true,
  "browserslist": [
    "last 4 version",
    "> 0.5% in JP"
  ],
  "prepare": "husky install",
  "lint-staged": {
    "*.{vue,ts}": [
      "yarn lint:script --fix",
      "yarn lint:style --fix"
    ]
  },
  "scripts": {
    "dev": "cross-env development=vite-ssg:* vite",
    "build": "cross-env production=vite-ssg:* vite-ssg build",
    "serve": "vite preview",
    "lint:markup": "vue-tsc --noEmit",
    "lint:script": "eslint --ext .ts,vue --ignore-path .gitignore .",
    "lint:style": "stylelint src/**/*.{css,vue}",
    "prepare": "husky install",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "dependencies": {
    "pinia": "2.0.11",
    "vite-ssg": "^0.17.9",
    "vue": "^3.2.29",
    "what-input": "^5.2.10"
  },
  "devDependencies": {
    "@nuxt/devalue": "^2.0.0",
    "@types/node": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "@vitejs/plugin-vue": "^2.1.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vueuse/head": "^0.7.5",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.16",
    "cssnano-preset-advanced": "^5.1.11",
    "eslint": "^8.8.0",
    "eslint-plugin-vue": "^8.4.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.3",
    "path": "^0.12.7",
    "postcss": "^8.4.6",
    "postcss-html": "^1.3.0",
    "postcss-import": "^14.0.2",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^7.3.1",
    "postcss-rem": "^2.0.2",
    "postcss-reporter": "^7.0.5",
    "postcss-sort-media-queries": "^4.2.1",
    "postcss-webfont": "^4.0.0",
    "stylelint": "^14.3.0",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-property-sort-order-smacss": "^8.0.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.5.5",
    "unplugin-vue-components": "^0.17.14",
    "vite": "^2.7.13",
    "vite-plugin-pages": "^0.20.0",
    "vite-plugin-vue-layouts": "^0.5.0",
    "vue-router": "^4.0.12",
    "vue-tsc": "^0.31.1"
  },
  "volta": {
    "node": "16.13.2"
  }
}
